{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "jsx": "react",
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist/",
      "sourceMap": true,
      "module": "commonjs",
      "target": "es6",
  }
}